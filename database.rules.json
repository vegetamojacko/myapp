{
  "rules": {
    ".read": false,
    ".write": false,
    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "claims": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        "$claimId": {
          ".write": "auth != null && auth.uid == $uid"
        }
      }
    },
    "carWashes": {
      ".read": "auth != null",
      ".write": false
    }
  }
}